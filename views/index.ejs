													<option value="23">28</option>
													<option value="23">29</option>
													<option value="23">30</option>
												</select>
											</div>

										</div>
										<button type="submit">Submit</button>
									</form>
									<div class="current-color-input">
										<label for="current-color" class="new">Current Temperature:</label>
